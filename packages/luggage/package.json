{
  "name": "@luggage/core",
  "version": "2.2.0",
  "description": "Dropbox file API wrapper for storing json data.",
  "main": "build/index.js",
  "scripts": {
    "test": "jest .",
    "lint": "eslint .",
    "build": "babel --source-maps inline --optional runtime -d build/ src/",
    "watch": "babel --watch --source-maps inline --optional runtime -d build/ src/",
    "build-browser": "webpack",
    "prepare": "yarn build && yarn build-browser",
    "prepublish": "yarn build && yarn build-browser && yarn test"
  },
  "directories": {
    "build": "build",
    "browser": "browser"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "author": "dra1n",
  "license": "MIT",
  "dependencies": {
    "deep-equal": "^1.0.0",
    "dropbox": "^4.0.7",
    "dropbox-client": "^3.0.1",
    "events": "^2.0.0",
    "lorgnette": "^1.2.0",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "jest": "^22.4.2"
  }
}
