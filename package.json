{
  "name": "luggage",
  "version": "2.1.12",
  "description": "Dropbox file API wrapper for storing json data.",
  "main": "browser/index.js",
  "scripts": {
    "test": "mocha",
    "test-watch": "mocha --watch",
    "build": "babel --source-maps inline --optional runtime -d lib/ src/",
    "build-browser": "webpack",
    "watch": "babel --watch --source-maps inline --optional runtime -d lib/ src/",
    "prepare": "npm run build && npm run build-browser",
    "prepublish": "npm run build && npm run build-browser && npm test",
    "lint": "eslint src"
  },
  "directories": {
    "lib": "lib",
    "browser": "browser"
  },
  "author": "dra1n",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.6.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.0.0",
    "chai": "^3.2.0",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^6.10.3",
    "mocha": "^3.2.0",
    "webpack": "^2.7.0"
  },
  "dependencies": {
    "deep-equal": "^1.0.0",
    "dropbox-client": "^3.0.1",
    "events": "^1.1.1"
  }
